<Window x:Class="Monke.AcbWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="ACB Operations" Height="197" Width="300">
    <StackPanel Margin="0,0,0,8">
        <Button Name="decompileAcbButton" Content="Decompile ACB" Margin="10" Click="DecompileAcbButton_Click"/>
        <Button Name="compileAcbButton" Content="Compile ACB" Margin="10" Click="CompileAcbButton_Click"/>
        <Button Name="adjustAcbVolumeButton" Content="Adjust ACB Volume" Margin="10" Click="AdjustAcbVolumeButton_Click"/>
    </StackPanel>
</Window>
